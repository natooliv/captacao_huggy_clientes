{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport EditContactModal from './EditContactModal';\nimport AddContactForm from './AddContactForm';\nexport default {\n  name: 'ContactList',\n  components: {\n    EditContactModal,\n    AddContactForm\n  },\n  data() {\n    return {\n      searchQuery: '',\n      contacts: [],\n      selectedContact: null,\n      showAddModal: false\n    };\n  },\n  computed: {\n    filteredContacts() {\n      return this.searchQuery.length > 0 ? this.contacts.filter(contact => contact.name.toLowerCase().includes(this.searchQuery.toLowerCase())) : this.contacts;\n    }\n  },\n  methods: {\n    openEditModal(contact) {\n      this.selectedContact = {\n        ...contact\n      };\n    },\n    updateContact(updatedContact) {\n      const index = this.contacts.findIndex(contact => contact.id === updatedContact.id);\n      if (index !== -1) {\n        this.contacts.splice(index, 1, updatedContact);\n      }\n      this.selectedContact = null;\n    },\n    openAddModal() {\n      this.showAddModal = true;\n    },\n    handleContactAdded(newContact) {\n      this.contacts.push(newContact);\n      this.showAddModal = false;\n    }\n  },\n  mounted() {\n    this.fetchContacts();\n  }\n};","map":{"version":3,"names":["axios","EditContactModal","AddContactForm","name","components","data","searchQuery","contacts","selectedContact","showAddModal","computed","filteredContacts","length","filter","contact","toLowerCase","includes","methods","openEditModal","updateContact","updatedContact","index","findIndex","id","splice","openAddModal","handleContactAdded","newContact","push","mounted","fetchContacts"],"sources":["/home/natalia/teste_huggy_/huggy/src/components/ContactList.vue"],"sourcesContent":["<template>\n  <div>\n    <input type=\"search\" placeholder=\"Buscar contato\" v-model=\"searchQuery\">\n    <table v-if=\"filteredContacts.length > 0\">\n      <thead>\n        <tr>\n          <th>Nome</th>\n          <th>Email</th>\n          <th>Telefone</th>\n          <th>Ações</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr v-for=\"contact in filteredContacts\" :key=\"contact.id\">\n          <td>{{ contact.name }}</td>\n          <td>{{ contact.email }}</td>\n          <td>{{ contact.phone }}</td>\n          <td>{{ contact.photo }}</td>\n          <td>\n            <button @click=\"openEditModal(contact)\">Editar</button>\n          </td>\n        </tr>\n      </tbody>\n    </table>\n    <EditContactModal v-if=\"selectedContact\" :contact=\"selectedContact\" @update=\"updateContact\" />\n    <AddContactForm v-if=\"showAddModal\" @contactAdded=\"handleContactAdded\" />\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport EditContactModal from './EditContactModal';\nimport AddContactForm from './AddContactForm';\n\nexport default {\n  name: 'ContactList',\n  components: {\n    EditContactModal,\n    AddContactForm,\n  },\n  data() {\n    return {\n      searchQuery: '',\n      contacts: [],\n      selectedContact: null,\n      showAddModal: false,\n    };\n  },\n  computed: {\n    filteredContacts() {\n      return this.searchQuery.length > 0\n        ? this.contacts.filter(contact =>\n            contact.name.toLowerCase().includes(this.searchQuery.toLowerCase()))\n        : this.contacts;\n    },\n  },\n  methods: {\n    openEditModal(contact) {\n      this.selectedContact = { ...contact }; \n    },\n    updateContact(updatedContact) {\n      const index = this.contacts.findIndex(contact => contact.id === updatedContact.id);\n      if (index !== -1) {\n        this.contacts.splice(index, 1, updatedContact);\n      }\n      this.selectedContact = null;\n    },\n    openAddModal() {\n      this.showAddModal = true;\n    },\n    handleContactAdded(newContact) {\n      this.contacts.push(newContact);\n      this.showAddModal = false;\n    },\n  },\n  mounted() {\n    this.fetchContacts();\n  },\n};\n</script>\n\n<style scoped>\n/* Estilos para ContactList */\n</style>\n"],"mappings":";AA8BA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,gBAAe,MAAO,oBAAoB;AACjD,OAAOC,cAAa,MAAO,kBAAkB;AAE7C,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IACVH,gBAAgB;IAChBC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,QAAQ,EAAE,EAAE;MACZC,eAAe,EAAE,IAAI;MACrBC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,gBAAgBA,CAAA,EAAG;MACjB,OAAO,IAAI,CAACL,WAAW,CAACM,MAAK,GAAI,IAC7B,IAAI,CAACL,QAAQ,CAACM,MAAM,CAACC,OAAM,IACzBA,OAAO,CAACX,IAAI,CAACY,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAACV,WAAW,CAACS,WAAW,CAAC,CAAC,CAAC,IACrE,IAAI,CAACR,QAAQ;IACnB;EACF,CAAC;EACDU,OAAO,EAAE;IACPC,aAAaA,CAACJ,OAAO,EAAE;MACrB,IAAI,CAACN,eAAc,GAAI;QAAE,GAAGM;MAAQ,CAAC;IACvC,CAAC;IACDK,aAAaA,CAACC,cAAc,EAAE;MAC5B,MAAMC,KAAI,GAAI,IAAI,CAACd,QAAQ,CAACe,SAAS,CAACR,OAAM,IAAKA,OAAO,CAACS,EAAC,KAAMH,cAAc,CAACG,EAAE,CAAC;MAClF,IAAIF,KAAI,KAAM,CAAC,CAAC,EAAE;QAChB,IAAI,CAACd,QAAQ,CAACiB,MAAM,CAACH,KAAK,EAAE,CAAC,EAAED,cAAc,CAAC;MAChD;MACA,IAAI,CAACZ,eAAc,GAAI,IAAI;IAC7B,CAAC;IACDiB,YAAYA,CAAA,EAAG;MACb,IAAI,CAAChB,YAAW,GAAI,IAAI;IAC1B,CAAC;IACDiB,kBAAkBA,CAACC,UAAU,EAAE;MAC7B,IAAI,CAACpB,QAAQ,CAACqB,IAAI,CAACD,UAAU,CAAC;MAC9B,IAAI,CAAClB,YAAW,GAAI,KAAK;IAC3B;EACF,CAAC;EACDoB,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,aAAa,CAAC,CAAC;EACtB;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}