{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport EditContactModal from './EditContactModal';\nimport AddContactForm from './AddContactForm';\nexport default {\n  name: 'ContactList',\n  components: {\n    EditContactModal,\n    AddContactForm\n  },\n  data() {\n    return {\n      searchQuery: '',\n      contacts: [],\n      selectedContact: null,\n      showAddModal: false\n    };\n  },\n  computed: {\n    filteredContacts() {\n      return this.searchQuery.length > 0 ? this.contacts.filter(contact => contact.name.toLowerCase().includes(this.searchQuery.toLowerCase())) : this.contacts;\n    }\n  },\n  methods: {\n    fetchContacts() {\n      axios.get('http://localhost:8000/api/clientes').then(response => {\n        this.contacts = Array.isArray(response.data) ? response.data : [];\n      }).catch(error => {\n        console.error('Erro ao buscar os clientes:', error);\n        this.contacts = [];\n      });\n    },\n    openDetailModal(contact) {\n      this.selectedContact = contact;\n    },\n    openEditModal(contact) {\n      this.selectedContact = {\n        ...contact\n      };\n    },\n    closeDetailModal() {\n      this.selectedContact = null;\n    },\n    updateContact(updatedContact) {\n      const index = this.contacts.findIndex(contact => contact.id === updatedContact.id);\n      if (index !== -1) {\n        this.contacts.splice(index, 1, updatedContact);\n      }\n      this.selectedContact = null;\n    },\n    getPhotoUrl(photoPath) {\n      // Construa o URL completo da foto usando o caminho relativo da foto\n      return 'http://localhost:8000/uploads/' + photoPath;\n    },\n    deleteContact(contactId) {\n      axios.delete(`http://localhost:8000/api/clientes/${contactId}`).then(response => {\n        console.log('Contato excluído com sucesso:', response.data);\n        this.contacts = this.contacts.filter(contact => contact.id !== contactId);\n      }).catch(error => {\n        console.error('Erro ao excluir contato:', error);\n      });\n    },\n    handleContactAdded(newContact) {\n      this.contacts.push(newContact);\n      this.showAddModal = false;\n    }\n  },\n  mounted() {\n    this.fetchContacts();\n  }\n};","map":{"version":3,"names":["axios","EditContactModal","AddContactForm","name","components","data","searchQuery","contacts","selectedContact","showAddModal","computed","filteredContacts","length","filter","contact","toLowerCase","includes","methods","fetchContacts","get","then","response","Array","isArray","catch","error","console","openDetailModal","openEditModal","closeDetailModal","updateContact","updatedContact","index","findIndex","id","splice","getPhotoUrl","photoPath","deleteContact","contactId","delete","log","handleContactAdded","newContact","push","mounted"],"sources":["/home/natalia/teste_huggy_/huggy/src/components/ContactList.vue"],"sourcesContent":["<template>\n  <div class=\"contact-list-container\">\n    <!-- Seção de busca e botão de adicionar contato -->\n    <div class=\"search-and-add-container\">\n      <div class=\"search-input-container\">\n        <input type=\"search\" class=\"search-input\" placeholder=\"Buscar contato\" v-model=\"searchQuery\">\n        <i class=\"bi bi-search search-icon\"></i>\n      </div>\n      <!-- Botão para acionar o modal de adicionar novo contato -->\n      <button class=\"add-contact-button\" @click=\"showAddModal = true\">\n        + Adicionar contato\n      </button>\n    </div>\n<!-- Aqui começa o novo modal de detalhes do contato -->\n<div v-if=\"selectedContact\" class=\"contact-detail-modal\">\n      <div class=\"modal-content\">\n        <h3>{{ selectedContact.name }}</h3>\n        <p><strong>Email:</strong> {{ selectedContact.email }}</p>\n        <p><strong>Endereço:</strong> {{ selectedContact.address }}</p>\n        <p><strong>Bairro:</strong> {{ selectedContact.neighborhood }}</p>\n        <p><strong>Cidade:</strong> {{ selectedContact.city }}</p>\n        <p><strong>Estado:</strong> {{ selectedContact.state }}</p>\n        <button @click=\"closeDetailModal\">Fechar</button>\n      </div>\n    </div>\n    <!-- Tabela de contatos com novo layout -->\n    <table v-if=\"filteredContacts.length > 0\" class=\"contacts-table\">\n      <div class=\"contacts-table\">\n      <div class=\"contact-header\">\n        <span>Nome <i class=\"fa fa-chevron-down\" aria-hidden=\"true\"></i></span>\n        <span>Email</span>\n        <span class=\"contact-header-phone\">Telefone</span>\n      </div>\n      <div class=\"contact-body\" v-for=\"contact in filteredContacts\" :key=\"contact.id\">\n        <div class=\"contact-row\" v-for=\"contact in filteredContacts\" :key=\"contact.id\" @click=\"openDetailModal(contact)\">\n          <div class=\"contact-photo\">\n            <img :src=\"getPhotoUrl(contact.photo)\" alt=\"Foto do contato\">\n          </div>\n          <div class=\"contact-name\">{{ contact.name }}</div>\n          <div class=\"contact-email\">{{ contact.email }}</div>\n          <div class=\"contact-phone\">{{ contact.phone }}</div>\n          <div class=\"contact-actions\">\n  <!-- Botão de editar com ícone de lápis -->\n  <button class=\"edit-button\"  id=\"fa fa-pencil-alt edit-icon\" @click=\"openEditModal(contact)\" title=\"Editar\"></button>\n  <!-- Botão de excluir com ícone de lixeira -->\n  <button class=\"delete-button\"  id=\"fa fa-trash delete-icon\" @click=\"deleteContact(contact.id)\" title=\"Excluir\"></button>\n</div>\n\n        </div>\n      </div>\n      </div>  \n    </table>\n\n    <!-- Mensagem de 'sem contatos' e botão de adicionar, se não houver contatos -->\n    <div v-else class=\"no-contacts\">\n      <img src=\"path_to_your_no_contacts_image.png\" alt=\"Sem contatos\" class=\"no-contacts-image\">\n      <p>Ainda não há contatos</p>\n    </div>\n    <div class=\"contact-actions\">\n    <!-- Modal de adicionar contato -->\n    <add-contact-form v-if=\"showAddModal\" @contactAdded=\"handleContactAdded\" @closeModal=\"showAddModal = false\" />\n\n    <!-- Modal de editar contato -->\n    <edit-contact-modal v-if=\"selectedContact\" :contact=\"selectedContact\" @update=\"updateContact\" />\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport EditContactModal from './EditContactModal';\nimport AddContactForm from './AddContactForm';\n\nexport default {\n  name: 'ContactList',\n  components: {\n    EditContactModal,\n    AddContactForm,\n  },\n  data() {\n    return {\n      searchQuery: '',\n      contacts: [],\n      selectedContact: null,\n      showAddModal: false,\n    };\n  },\n  computed: {\n    filteredContacts() {\n      return this.searchQuery.length > 0\n        ? this.contacts.filter(contact =>\n            contact.name.toLowerCase().includes(this.searchQuery.toLowerCase()))\n        : this.contacts;\n    },\n  },\n  methods: {\n    fetchContacts() {\n      axios.get('http://localhost:8000/api/clientes')\n        .then(response => {\n          this.contacts = Array.isArray(response.data) ? response.data : [];\n        })\n        .catch(error => {\n          console.error('Erro ao buscar os clientes:', error);\n          this.contacts = [];\n        });\n    },\n    openDetailModal(contact) {\n      this.selectedContact = contact;\n    },\n    openEditModal(contact) {\n      this.selectedContact = { ...contact };\n    },\n    closeDetailModal() {\n      this.selectedContact = null;\n    },\n    updateContact(updatedContact) {\n      const index = this.contacts.findIndex(contact => contact.id === updatedContact.id);\n      if (index !== -1) {\n        this.contacts.splice(index, 1, updatedContact);\n      }\n      this.selectedContact = null;\n    },\n    getPhotoUrl(photoPath) {\n    // Construa o URL completo da foto usando o caminho relativo da foto\n    return 'http://localhost:8000/uploads/' + photoPath;\n  },\n    deleteContact(contactId) {\n      axios.delete(`http://localhost:8000/api/clientes/${contactId}`)\n        .then(response => {\n          console.log('Contato excluído com sucesso:', response.data);\n          this.contacts = this.contacts.filter(contact => contact.id !== contactId);\n        })\n        .catch(error => {\n          console.error('Erro ao excluir contato:', error);\n        });\n    },\n    handleContactAdded(newContact) {\n      this.contacts.push(newContact);\n      this.showAddModal = false;\n    },\n  },\n  mounted() {\n    this.fetchContacts();\n  },\n};\n</script>\n\n<style scoped>\n.search-and-add-container {\n  display: flex;\n  justify-content: space-between;\n  margin-bottom: 20px;\n}\n\n.search-input-container {\n  position: relative;\n  flex-grow: 1;\n  margin-right: 10px;\n}\n.contacts-table {\n  width: 100%;\n  margin-top: 20px;\n}\n\n.contact-header {\n  display: flex;\n  justify-content: space-between;\n  padding-bottom: 10px;\n  border-bottom: 2px solid #eee;\n  font-weight: bold;\n}\n\n.contact-body {\n  margin-top: 10px;\n}\n.contact-header-phone {\n  margin-left: -292px;\n  display: flex;\n    margin-right: 317px;\n\n}\n\n\n.contact-row {\n  display: flex;\n  align-items: center;\n  padding: 10px 0;\n  \n}\n\n.contact-photo img {\n  width: 40px; /* Ajuste o tamanho conforme necessário */\n  height: 40px;\n  border-radius: 50%;\n  object-fit: cover;\n  margin-right: 10px; /* Adiciona espaço entre a foto e o nome */\n}\n.contact-name{\n  flex: 1;\n    padding: 0 10px;\n    display: flex;\n}\n\n.contact-email{\n  flex: 1;\n  padding: 0 10px;\n}\n\n.contact-phone {\n  flex: 1;\n  padding: 0 10px;\n}\n\n.contact-actions {\n  display: flex;\n  justify-content: flex-end;\n}\n.search-input {\n  padding: 10px 10px 10px 35px; /* Espaço para o ícone */\n  border: 1px solid #ccc;\n  border-radius: 8px;\n  display: flex;\n  color: #E1E1E1;\n}\n.contact-detail-modal {\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background-color: rgba(0, 0, 0, 0.5);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.modal-content {\n  background: white;\n  padding: 20px;\n  border-radius: 8px;\n  text-align: left;\n}\n.search-icon {\n  position: absolute;\n  left: 10px; /* Ajuste conforme necessário */\n  top: 50%;\n  transform: translateY(-50%);\n  color: #9eabb8; /* Cor do ícone */\n  pointer-events: none; /* O ícone não afetará a interação com o input */\n  z-index: 5;\n}\n.contact-list-container {\n  padding: 50px;\n}\n\n.search-and-add-container {\n  display: flex;\n  justify-content: space-between;\n  margin-bottom: 20px;\n}\n\n\n.add-contact-button {\n  background-color: #321BDE;\n  color: white;\n  padding: 10px 20px;\n  border: none;\n  border-radius: 8px;\n  cursor: pointer;\n  font-weight: bold;\n}\n\n.contacts-table {\n  padding: 38px 15px;\n  border-collapse: collapse;\n}\n\n.contacts-table th, .contacts-table td {\n  padding: 10px;\n  \n}\n\n.edit-button, .delete-button {\n  padding: 5px 10px;\n  margin-right: 5px;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n}\n#edit-icon, .delete-icon {\n  padding: 5px;\n  margin-right: 5px;\n  color: #fff;\n  cursor: pointer;\n  font-size: 1em; /* ou o tamanho que preferir */\n}\n\n#edit-icon {\n  color: #000000;\n}\n\n#delete-icon {\n color: #000000;\n}\n\n#edit-icon:hover, .delete-icon:hover {\n  opacity: 0.8;\n}\n\n.edit-button {\n  background-color: #FFC107;\n  color: white;\n}\n\n.delete-button {\n  background-color: #F44336;\n  color: white;\n}\n\n.no-contacts {\n  text-align: center;\n  padding: 20px;\n}\n\n.no-contacts p {\n  margin-top: 16px;\n  margin-bottom: 24px;\n}\n\n.no-contacts-image {\n  max-width: 100px;\n  margin-bottom: 8px;\n}\n</style>\n"],"mappings":";AAqEA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,gBAAe,MAAO,oBAAoB;AACjD,OAAOC,cAAa,MAAO,kBAAkB;AAE7C,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IACVH,gBAAgB;IAChBC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,QAAQ,EAAE,EAAE;MACZC,eAAe,EAAE,IAAI;MACrBC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,gBAAgBA,CAAA,EAAG;MACjB,OAAO,