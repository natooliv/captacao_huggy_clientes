{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport EditContactModal from './EditContactModal';\nimport AddContactForm from './AddContactForm';\nexport default {\n  name: 'ContactList',\n  components: {\n    EditContactModal,\n    AddContactForm\n  },\n  data() {\n    return {\n      searchQuery: '',\n      contacts: [],\n      selectedContact: null,\n      showAddModal: false\n    };\n  },\n  computed: {\n    filteredContacts() {\n      return this.searchQuery.length > 0 ? this.contacts.filter(contact => contact.name.toLowerCase().includes(this.searchQuery.toLowerCase())) : this.contacts;\n    }\n  },\n  methods: {\n    fetchContacts() {\n      axios.get('http://localhost:8000/api/clientes').then(response => {\n        this.contacts = Array.isArray(response.data) ? response.data : [];\n      }).catch(error => {\n        console.error('Erro ao buscar os clientes:', error);\n        this.contacts = [];\n      });\n    },\n    openEditModal(contact) {\n      this.selectedContact = {\n        ...contact\n      };\n    },\n    updateContact(updatedContact) {\n      const index = this.contacts.findIndex(contact => contact.id === updatedContact.id);\n      if (index !== -1) {\n        this.contacts.splice(index, 1, updatedContact);\n      }\n      this.selectedContact = null;\n    },\n    deleteContact(contactId) {\n      axios.delete(`http://localhost:8000/api/clientes/${contactId}`).then(response => {\n        console.log('Contato excluído com sucesso:', response.data);\n        this.contacts = this.contacts.filter(contact => contact.id !== contactId);\n      }).catch(error => {\n        console.error('Erro ao excluir contato:', error);\n      });\n    },\n    handleContactAdded(newContact) {\n      this.contacts.push(newContact);\n      this.showAddModal = false;\n    }\n  },\n  mounted() {\n    this.fetchContacts();\n  }\n};","map":{"version":3,"names":["axios","EditContactModal","AddContactForm","name","components","data","searchQuery","contacts","selectedContact","showAddModal","computed","filteredContacts","length","filter","contact","toLowerCase","includes","methods","fetchContacts","get","then","response","Array","isArray","catch","error","console","openEditModal","updateContact","updatedContact","index","findIndex","id","splice","deleteContact","contactId","delete","log","handleContactAdded","newContact","push","mounted"],"sources":["/home/natalia/teste_huggy_/huggy/src/components/ContactList.vue"],"sourcesContent":["<template>\n  <div>\n    <input type=\"search\" placeholder=\"Buscar contato\" v-model=\"searchQuery\">\n    <table v-if=\"filteredContacts.length > 0\">\n      <thead>\n        <tr>\n          <th>Nome</th>\n          <th>Email</th>\n          <th>Telefone</th>\n          <th>Ações</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr v-for=\"contact in filteredContacts\" :key=\"contact.id\">\n          <td>{{ contact.name }}</td>\n          <td>{{ contact.email }}</td>\n          <td>{{ contact.phone }}</td>\n          <td>\n            <button @click=\"openEditModal(contact)\">Editar</button>\n            <button @click=\"deleteContact(contact.id)\">Excluir</button> <!-- Botão de exclusão -->\n          </td>\n        </tr>\n      </tbody>\n    </table>\n    <EditContactModal v-if=\"selectedContact\" :contact=\"selectedContact\" @update=\"updateContact\" />\n    <AddContactForm v-if=\"showAddModal\" @contactAdded=\"handleContactAdded\" />\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport EditContactModal from './EditContactModal';\nimport AddContactForm from './AddContactForm';\n\nexport default {\n  name: 'ContactList',\n  components: {\n    EditContactModal,\n    AddContactForm,\n  },\n  data() {\n    return {\n      searchQuery: '',\n      contacts: [],\n      selectedContact: null,\n      showAddModal: false,\n    };\n  },\n  computed: {\n    filteredContacts() {\n      return this.searchQuery.length > 0\n        ? this.contacts.filter(contact =>\n            contact.name.toLowerCase().includes(this.searchQuery.toLowerCase()))\n        : this.contacts;\n    },\n  },\n  methods: {\n    fetchContacts() {\n      axios.get('http://localhost:8000/api/clientes')\n        .then(response => {\n          this.contacts = Array.isArray(response.data) ? response.data : [];\n        })\n        .catch(error => {\n          console.error('Erro ao buscar os clientes:', error);\n          this.contacts = [];\n        });\n    },\n    openEditModal(contact) {\n      this.selectedContact = { ...contact }; \n    },\n    updateContact(updatedContact) {\n      const index = this.contacts.findIndex(contact => contact.id === updatedContact.id);\n      if (index !== -1) {\n        this.contacts.splice(index, 1, updatedContact);\n      }\n      this.selectedContact = null;\n    },\n    deleteContact(contactId) {\n      axios.delete(`http://localhost:8000/api/clientes/${contactId}`)\n        .then(response => {\n          console.log('Contato excluído com sucesso:', response.data);\n          this.contacts = this.contacts.filter(contact => contact.id !== contactId);\n        })\n        .catch(error => {\n          console.error('Erro ao excluir contato:', error);\n        });\n    },\n    handleContactAdded(newContact) {\n      this.contacts.push(newContact);\n      this.showAddModal = false;\n    },\n  },\n  mounted() {\n    this.fetchContacts();\n  },\n};\n</script>\n\n<style scoped>\n/* Estilos para ContactList */\n</style>\n"],"mappings":";AA8BA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,gBAAe,MAAO,oBAAoB;AACjD,OAAOC,cAAa,MAAO,kBAAkB;AAE7C,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IACVH,gBAAgB;IAChBC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,QAAQ,EAAE,EAAE;MACZC,eAAe,EAAE,IAAI;MACrBC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,gBAAgBA,CAAA,EAAG;MACjB,OAAO,IAAI,CAACL,WAAW,CAACM,MAAK,GAAI,IAC7B,IAAI,CAACL,QAAQ,CAACM,MAAM,CAACC,OAAM,IACzBA,OAAO,CAACX,IAAI,CAACY,WAAW,CAAC,CAAC,CAACC,